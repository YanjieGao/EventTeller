<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="db.hbn.model">
	<class
		name="Event"
		table="Event" dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		</id>
		<property
			name="title"
			column="title"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="pubTime"
			column="pubtime"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="content"
			column="content"
			type="string"
			not-null="false"
		/>
		<property
			name="source"
			column="source"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="imgs"
			column="imgs"
			type="string"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>