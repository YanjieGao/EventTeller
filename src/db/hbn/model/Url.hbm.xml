<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="db.hbn.model">
	<class
		name="Url"
		table="Url" dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		</id>
		<property
			name="webSite"
			column="webSite"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="url"
			column="url"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="crawlTime"
			column="crawltime"
			type="java.util.Date"
			not-null="true"
			length="14"
		/>
		<property
			name="subtopicId"
			column="subtopicid"
			type="integer"
			not-null="false"
			length="8"
		/>
	</class>	
</hibernate-mapping>