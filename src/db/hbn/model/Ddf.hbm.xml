<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="db.hbn.model">
	<class
		name="Ddf"
		table="ddf" dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		<generator class="native"></generator>
		</id>
		<property
			name="day"
			column="day"
			type="string"
		/>
		<property
			name="docnum"
			column="docnum"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="wordnum"
			column="wordnum"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="words"
			column="words"
			type="string"
		/>
		<property
			name="time"
			column="updatetime"
			type="java.util.Date"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>