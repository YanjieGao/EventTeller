<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="db.hbn.model">
	<class
		name="EventInfo"
		table="EventInfo" dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		</id>
		<property
			name="number"
			column="number"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="day"
			column="day"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="topic"
			column="topic"
			type="integer"
			not-null="false"
			length="32"
		/>
	</class>	
</hibernate-mapping>