<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="db.hbn.model">
	<class
		name="Article"
		table="article" dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		</id>
		<property
			name="title"
			column="title"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="url"
			column="url"
			type="string"
			not-null="true"
			length="300"
		/>
		<property
			name="publishtime"
			column="pubtime"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="content"
			column="content"
			type="string"
			not-null="false"
		/>
		<property
			name="crawltime"
			column="crawltime"
			type="java.util.Date"
			not-null="true"
			length="14"
		/>
		<property
			name="imgs"
			column="imgs"
			type="string"
			not-null="false"
		/>
		<property
			name="source"
			column="source"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="topicid"
			column="topicid"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="taskstatus"
			column="taskstatus"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="subtopic"
			column="subtopic"
			type="integer"
			not-null="false"
			length="32"
		/>
		<property
			name="sameurls"
			column="sameurls"
			type="string"
			not-null="false"
		/>
		<property
			name="number"
			column="number"
			type="integer"
			not-null="false"
			length="32"
		/>
	</class>	
</hibernate-mapping>